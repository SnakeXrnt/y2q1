[apple, banana, cherry] ===> (apple banana cherry)
[apple, banana] ===> (apple banana ()) OR (() apple banana)
[apple] ===> apple
[] ===> ()
[apple, banana, cherry, date] ===> (apple banana (() cherry date)) OR ((apple banana ()) cherry date)
[apple, banana, cherry, date, elderberry] ===> ((apple banana ()) cherry (date elderberry ())) OR ((() apple banana) cherry (() date elderberry))
[apple, banana, cherry, date, elderberry, fig, grape] ===> ((apple banana cherry) date (elderberry fig grape))
